{
    "name": "supen/alipay",
    "minimum-stability": "dev",
    "description": "支付宝SDK封装",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-SimpleXML": "*",
        "pimple/pimple": "^3.2@dev",
        "symfony/cache": "^5.1@dev",
        "symfony/http-foundation": "^v5.0.5",
        "symfony/psr-http-message-bridge": "^1.3@dev",
        "supen/supports": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supen\\Alipay\\": "src"
        },
        "classmap": [
            "sdk/aop"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Supen\\Alipay\\Provider\\LaravelServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../supports",
            "options": {
                "symlink": true
            }
        }
    ],
    "authors": [
        {
            "name": "Supen.Huang",
            "email": "supen.huang@qq.com"
        }
    ]
}
